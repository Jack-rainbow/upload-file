{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "paths": {
    "~/*": [
      "./src/*"
    ],
    "@/*": [
      "./src/*"
    ]
  },
  "rules": {
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-bitwise": false,
    "no-console": false,
    "no-empty": false,
    "noImplicitAny": false,
    "quotemark": false
  }
}
